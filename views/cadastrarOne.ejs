<%- include('partials/header.ejs'); %>
<%- include('partials/nav.ejs'); %>  
<main class="main-container">
    <form action="" class="container-form">
        <h2>Faça seu cadastro</h2>
        <label for="namePlace">Nome: <input type="text" class="inputs-Cadastro" placeholder=" Insira seu nome" id="namePlace" name="namePlace" minlength="4" maxlength="30" required></label>
        <label for="emailPlace">E-mail: <input type="email" class="inputs-Cadastro" placeholder=" Insira seu e-mail" id="emailPlace" name="emailPlace" required></label>
        <label for="CPFPlace">CPF: <input type="text" class="inputs-Cadastro" placeholder=" Insira seu CPF" class="cpf" id="CPFPlace" name="CPFPlace" maxlength="14" required></label>
        <div class="areaNascimento">
            <label for="dataPlace">Data de nascimento: <input type="date" class="inputs-Cadastro" placeholder=" Dia/Mês/Ano" id="dataPlace"name="dataPlace" required></label>
            <label for="senhaPlace">Senha: <input type="password" class="inputs-Cadastro" id="senhaPlace" name="senhaPlace" placeholder=" Insira sua senha" required></label>
            <label for="repeatSenhaPlace">Insira novamente sua senha:<input type="password" class="inputs-Cadastro" placeholder="Digite sua senha novamente" name="repeatSenhaPlace" id="repeatSenhaPleace"></label>
            <input class="termosDeCondicoes" type="checkbox" required><a class="linkTermos" href="#">Aceito os termos de condições</a>
        </div>
        <button class="btnProsseguir" type="submit">Prosseguir</button>
    </form>
</main>
<%- include('partials/footer.ejs'); %>  